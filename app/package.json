{
  "name": "session-timer",
  "private": true,
  "version": "2.0.0",
  "description": "Machiasobi Signage System Frontend",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "build": "npm run compile:js && npm run compile:css",
    "deploy": "npm run build && gsutil rsync public/ gs://web.timer.builderscon.io/ && gsutil acl ch -u AllUsers:R gs://web.timer.builderscon.io/*",
    "compile:js": "webpack",
    "compile:css": "node-sass --output-style compressed src/stylesheets/index.scss > public/all.css",
    "test": "mocha --compilers js:babel-core/register"
  },
  "dependencies": {
    "express": "^4.13.4",
    "lodash": "^4.13.1",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-modal-dialog": "^3.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-logger": "^2.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "expect": "^1.20.1",
    "expect-jsx": "^2.5.1",
    "jsdom": "^9.2.1",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "react-addons-test-utils": "^15.1.0",
    "sinon": "^1.17.4",
    "webpack": "^1.13.1"
  }
}
